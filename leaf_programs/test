struct User {
    name;
    age;

    __construct() {
    };
};

const get_name = function (self) {
    return self.name;
};

const get_age = function (self) {
    return self.age;
};

const set_name = function (self, name) {
    self.name = name;
    return self;
};

const set_age = function (self, age) {
    self.age = age;
    return self;
};

const user = User();

set_age(
    set_name(user, "Dinosaure"),
    765
);

println user;
